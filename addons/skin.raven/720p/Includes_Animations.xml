<includes>
  <include name="Animation_Jukebox_AlbumList">
    <animation effect="slide" start="0,270" end="0,0" time="700" tween="cubic" easing="out">WindowOpen</animation>
    <animation effect="slide" start="0,0" end="0,270" time="700" tween="cubic" easing="in">WindowClose</animation>
    <animation effect="slide" start="0,270" end="0,0" time="700" tween="cubic" easing="out" reversible="false">Visible</animation>
    <animation effect="slide" start="0,0" end="0,270" time="700" tween="cubic" easing="in" delay="8000">Hidden</animation>
  </include>
  <include name="Animation_Jukebox_NowPlaying">
